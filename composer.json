{
    "name": "ycs77/laravel-crud-page",
    "description": "Fast generate CRUD pages, controllers of the Laravel.",
    "type": "library",
    "keywords": [
        "laravel",
        "crud",
        "generator",
        "pages",
        "controllers"
    ],
    "require": {
        "kabbouchi/laravel-table-view": "^0.1.15",
        "kris/laravel-form-builder": "^1.20",
        "ycs77/laravel-form-builder-bs4": "^1.0",
        "ycs77/laravel-form-field-type": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "orchestra/testbench": "^3.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Yang",
            "email": "yangchenshin77@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ycs77\\LaravelCrudPage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ycs77\\LaravelCrudPage\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Ycs77\\LaravelCrudPage\\CrudPageServiceProvider"
            ],
            "aliases": {
                "CrudPage": "Ycs77\\LaravelCrudPage\\Facades\\CrudPage"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
